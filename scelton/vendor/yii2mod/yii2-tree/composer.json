    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "tree",
        "tree widget"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoi",
            "email": "chepurnoi.igor@gmail.com"
        }
    ],
    "require": {
